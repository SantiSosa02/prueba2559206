{{> navbar}}
<link rel="stylesheet" href="../css/modulos.css">
<div class="total">
  <div class="caja">
    <h1>Detalle venta</h1>
    <div class="contDetalle">
      <br><br>
      <h3>Productos</h3>
      <div class="table-container">

        <table class="table table-striped-columns">
          <thead>
            <tr>
              <th scope="col">Producto</th>
              <th scope="col">Cantidad</th>
              <th scope="col">Precio</th>
            </tr>
          </thead>
          <tbody>
            {{#each lista_productosVenta}}
            <tr>
              <td>{{producto}}</td>
              <td>{{cantidad}}</td>
              <td>{{precio}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
      <br><br><br>
      <h3>Servicios</h3>
      <div class="table-container">

        <table class="table table-striped-columns">
          <thead>
            <tr>
              <th scope="col">Servicio</th>
              <th scope="col">Cantidad</th>
              <th scope="col">Precio</th>
            </tr>
          </thead>
          <tbody>
            {{#each lista_serviciosVenta}}
            <tr>
              <td>{{servicio}}</td>
              <td>{{cantidad}}</td>
              <td>{{precio}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
      </div>
      </div>
    </div>
    {{> footer}}